{
  "stage": 0,
  "env": {
    "development": {
      "optional": ["runtime"],
      "plugins": [ "react-transform" ],
      "extra": {
        "react-transform": {
          "transforms": [{
            "transform": "livereactload/babel-transform",
            "imports": ["react"]
          }]
        }
      }
    },
    "production": {
      "optional": ["runtime"]
    }
  }
}
